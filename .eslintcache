roup,\r\n          }));\r\n\r\n          this.setState({ products: packages_, totalPrice: total });\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    }\r\n  };\r\n\r\n  sendOrder = () => {\r\n    let order = {};\r\n    order[\"orderInfo\"] = {\r\n      transport: 1,\r\n      status: 0,\r\n      payment: this.state.payment,\r\n      user_id: this.state.userInfo.id,\r\n      totalprice: this.state.totalPrice,\r\n    };\r\n\r\n    order[\"products\"] = this.state.productList;\r\n\r\n    console.log(order);\r\n\r\n    if (this.state.token) {\r\n      callAPI(\"/checkout/insertOder\", \"POST\", order, this.state.token)\r\n        .then((res) => {\r\n          console.log(\"add oder successful\");\r\n          this.setState({ redirect: \"/orders\" });\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    }\r\n\r\n    let data = {};\r\n    data[\"productids\"] = this.state.productList.map((p) => p.id);\r\n\r\n    //delete old products from cart\r\n    if (this.state.token) {\r\n      callAPI(\"/info/deleteCartCheckout\", \"POST\", data, this.state.token)\r\n        .then((res) => {\r\n          console.log(\"add oder successful\");\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    }\r\n  };\r\n\r\n  handleChangePayment = (e) => {\r\n    this.setState({ payment: e.target.value });\r\n    console.log(this.state.payment);\r\n  };\r\n\r\n  getCheckoutData = () => {\r\n    this.getProducts();\r\n    this.getUserInfo();\r\n  };\r\n\r\n  componentWillMount = () => {\r\n    let token_ = localStorage.getItem(\"token\");\r\n\r\n    this.setState({ token: token_ }, () => {\r\n      this.getCheckoutData();\r\n    });\r\n  };\r\n\r\n  render() {\r\n    let { redirect } = this.state;\r\n    if (redirect) {\r\n      this.setState({ redirect: \"\" });\r\n      return <Redirect to={redirect} />;\r\n    }\r\n    const reciver = this.state.userInfo;\r\n    let all