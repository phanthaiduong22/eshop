>\n                      <div style={{ display: \"flex\", alignItems: \"center\" }}>\n                        <FontAwesomeIcon icon={faUser} className=\"fa-2x\" />\n                        <span\n                          style={{ fontSize: \"small\", paddingLeft: \"0.5em\" }}\n                        >\n                          Bạn chưa<br></br>\n                          đăng nhập\n                        </span>\n                      </div>\n                    </Link>\n                  </li>\n                </ul>\n              </div>\n            </div>\n          </nav>\n        </div>\n      </React.Fragment>\n    );\n  }\n  showMenus = (menus) => {\n    let result = null;\n    if (menus.length > 0) {\n      result = menus.map((menu, index) => {\n        return (\n          <MenuLink\n            key={index}\n            label={menu.name}\n            to={menu.to}\n            activeOnlyWhenExact={menu.exact}\n          />\n        );\n      });\n    }\n    return result;\n  };\n}\n\nexport default Menu;\n","/home/plh/Desktop/fenmcnpm/eshop/src/pages/Search/Search.js",[],"/home/plh/Desktop/fenmcnpm/eshop/src/pages/Checkout/Checkout.js",["95","96","97"],"import React, { Component } from \"react\";\n\nclass Checkout extends Component {\n  render() {\n    return (\n      <div className=\"container\">\n\n  <div className=\"row\">\n   <div className=\"col-md-6\">\n\n      <div class=\"card\">\n        <div class=\"card-body\">\n          <h4 class=\"card-title\">Title</h4>\n          <p class=\"card-text\">Text</p>\n\n          <button type=\"button\" name=\"\" id=\"\" class=\"btn btn-danger btn-lg btn-block\">CLick me</button>\n        </div>\n      </div>\n\n   </div>\n   <div className=\"col-md-6\">\n      <div class=\"card\">\n        <div class=\"card-body\">\n          <h4 class=\"card-title\">Title</h4>\n          <p class=\"card-text\">Text</p>\n          <div class=\"dropdown\">\n  <button class=\"btn btn-secondary dropdown-toggle\" type=\"button\" id